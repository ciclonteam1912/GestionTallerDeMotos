
@{
    ViewBag.Title = "Stock";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-lg-9 col-md-9 col-xs-11">
    <div class="container">
        <h1>Stock</h1>
        <div class="row">
            <div class="col-lg-3 col-md-3">
                <a href="~/Producto">
                    <div class="panel panel-primary modulo">

                        <div class="panel-heading text-center">
                            <div class="row">
                                <i class="fa fa-star fa-5x" id="inicioVentas"></i>
                            </div>
                            <div class="row">
                                <div class="">Productos y Servicios</div>
                            </div>
                        </div>

                    </div>
                </a>
            </div>
            <div class="col-lg-3 col-md-3">
                <a href="#">
                    <div class="panel panel-primary modulo">
                        <div class="panel-heading text-center">
                            <div class="row">
                                <i class="fa fa-cogs fa-5x" id="inicioVentas"></i>
                            </div>
                            <div class="row">
                                <div class="">Ajustes de Stock</div>
                            </div>
                        </div>
                    </div>
                </a>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    <script>
        $(document).ready(function () {
            $("#menuStock").addClass('active');

        });
    </script>
}

